{
  "name": "vite-template-redux",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "tsc": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"./src/**/*.{ts,tsx,js,jsx,json}\" --config .prettierrc.cjs",
    "tsc:watch": "tsc --watch",
    "tailwind:build": "npx tailwindcss -i ./src/index.css -o ./src/tailwind.css",
    "tailwind:watch": "npx tailwindcss -i ./src/index.css -o ./src/tailwind.css --watch",
    "svgr": "npx @svgr/cli ./src/assets/icons -d src/components/icons --typescript --icon --svg-props \"width=24,height=24\""
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.5",
    "axios": "^1.4.0",
    "fbemitter": "3.0.0",
    "i18next": "^23.2.6",
    "lodash": "4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.42.1",
    "react-i18next": "12.1.1",
    "react-redux": "8.1.1",
    "react-router-dom": "6.14.1",
    "react-select": "5.7.0",
    "redux-persist": "6.0.0",
    "socket.io-client": "^4.7.1",
    "tailwind-merge": "1.8.1",
    "vite-plugin-environment": "1.1.3"
  },
  "devDependencies": {
    "@svgr/cli": "6.5.1",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.2.5",
    "@types/fbemitter": "2.0.32",
    "@types/lodash": "^4.14.195",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.2.6",
    "@types/redux-persist": "4.3.1",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.43.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^21.1.0",
    "lodash": "^4.17.21",
    "postcss": "^8.4.24",
    "prettier": "2.8.4",
    "react-select-event": "5.5.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.0.0",
    "vitest": "^0.30.1"
  }
}
